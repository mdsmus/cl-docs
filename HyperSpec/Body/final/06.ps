       always \201< i 11\202\202
=>  T
 \201loop for i from 0 to 10
       never \201> i 11\202\202
=>  T
 
;; If I exceeds 10 return I; otherwise, return NIL.
;; The THEREIS construct terminates this loop.
 \201loop for i from 0
       thereis \201when \201> i 10\202 i\202 \202
=>  11

;;; The FINALLY clause is not evaluated in these examples.
 \201loop for i from 0 to 10
       always \201< i 9\202
       finally \201print "you won't see this"\202\202
=>  NIL
 \201loop never t
       finally \201print "you won't see this"\202\202
=>  NIL
 \201loop thereis "Here is my value"
       finally \201print "you won't see this"\202\202
=>  "Here is my value"
 
;; The FOR construct terminates this loop, so the FINALLY clause 
